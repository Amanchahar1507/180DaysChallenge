3.......Two Dimensional World

void twoDimensional(int *A[],int N){
    
    // Loop to iterate through matrix
    for(int i = 0;i<N;i++){
        for(int j = 0;j<N;j++){
            cout<<A[i][j]<<" ";
            
        }
        cout << endl;
    }
    
}

