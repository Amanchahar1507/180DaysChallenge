1. for(int i=1;i<=n*n;i++) 
       cout<<"Coder Army";
t.c = o(n^2).
s.c = o(1).

   
   2: for(int i=1;i<=n*n;i=i+2) 
       cout<<"Coder Army";
t.c = o(n^2)
s.c = o(1).
   
  3: for(int i=1;i<=n;i++) 
      for(int j=1;j<=n;j=j+5)
      cout<<"Coder Army";
t.c = o(n^2).
s.c = o(1)
  
  4: for(int i=1;i<=n;i++) 
      for(int j=i;j<=n;j=j++)
      cout<<"Coder Army";
t.c = o(n^2).
s.c = o(1).

  5: for(int i=1;i<=n;i++) 
      for(int j=1;j<=n;j=j*4)
      cout<<"Coder Army";
t.c = o(n*log(n)).
s.c = o(1)
  
  6: for(int i=1;i<=n;i=i*2) 
      for(int j=1;j<=i;j=j++)
      cout<<"Coder Army";
t.c = o(n).
s.c = o(1)

  7: for(int i=1;i<=n;i++) 
      for(int j=1;j<=n;j=j++)
      for(int k=1;k<=n;k=k*3)
      cout<<"Coder Army";
t.c = o(n^2*log(n)).
s.c = o(1)
 
  8: for(int i=1;i<=n;i++) 
      for(int j=1;j<=n;j=j++)
      for(int k=1;k<=n;k++)
      cout<<"Coder Army";
t.c = o(n^3).
s.c = o(1).

